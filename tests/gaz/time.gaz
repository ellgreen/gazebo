(load time)

(let i 20)

(let start (time.now))

(while (> i 0) (
  (println i)
  (let i (i[.-] 1))
  (time.sleep 30)
))

(let finish (time.now))

(println "took:" (- (num finish) (num start)) "seconds")

(fun timeit (f) (
  (let start (time.now))
  (f)
  (println "took:" (time.since start))
))

(fun test () (
  time.sleep 200
))

(timeit test)
