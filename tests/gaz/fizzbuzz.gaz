(let counter 0)
(
  while (< counter 100) (
    (
      if (! (% counter 15))
        (printf "%2d fizz buzz\n" counter)
      (if (! (% counter 3))
        (printf "%2d fizz\n" counter)
      (if (! (% counter 5))
        (printf "%2d buzz\n" counter)
        (println counter)
      ))
    )
    (let counter (+ counter 1))
  )
)
